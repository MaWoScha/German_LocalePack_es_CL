{
  "name": "mawoscha/german-localepack-es-cl",
  "type": "magento-module",
  "license":"OSL-3.0",
  "description":"Spanish (Chile) Community Translation",
  "authors":[
    {
      "name":"MaWoScha"
    }
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "magento-root-dir": "./",
    "magento-deploystrategy": "copy",
    "map": [
      ["./app/code/community/German/LocalePackCl", "./app/code/community/German/LocalePackCl"],
      ["./app/etc/modules/German_LocalePackCl.xml", "./app/etc/modules/German_LocalePackCl.xml"],
      ["./app/locale/de_DE/German_LocaleCl.csv", "./app/locale/de_DE/German_LocaleCl.csv"],
      ["./app/locale/es_CL/German_LocaleCl.csv", "./app/locale/es_CL/German_LocaleCl.csv"],
      ["./app/locale/es_ES/German_LocaleCl.csv", "./app/locale/es_ES/German_LocaleCl.csv"],
      ["./app/locale/pt_BR/German_LocaleCl.csv", "./app/locale/pt_BR/German_LocaleCl.csv"]
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
